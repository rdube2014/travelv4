<h2>Sign up</h2>



<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :first_name %><br />
  <%= f.text_field :first_name, :autofocus => true %></div>

  <div><%= f.label :last_name %><br />
  <%= f.text_field :last_name, :autofocus => true %></div>

  <div><%= f.label :home_city %><br />
  <%= f.text_field :home_city, :autofocus => true %></div>

  <div><%= f.label :home_state %><br />
  <%= f.text_field :home_state, :autofocus => true %></div>

  <div><%= f.label :home_country %><br />
  <%= f.text_field :home_country, :autofocus => true %></div>

  <div><%= f.label :photo_url %><br />
  <%= f.text_field :photo_url, :autofocus => true %></div>

  <div><%= f.label :email %><br />
  <%= f.email_field :email, :autofocus => true %></div>

  <div><%= f.label :user_name %><br />
  <%= f.text_field :user_name, :autofocus => true %></div>

  <div><%= f.label :password %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.submit "Sign up" %></div>
<% end %>

<%= render "devise/shared/links" %>
